{
  "build": {
    "beforeDevCommand": "npm run start -- --port 1420",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist/WAIUA-RE",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "WAIUA_RE",
    "version": "0.0.5"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "http": {
        "all": true,
        "request": true,
        "scope": [
          "https://valorant-api.com/**",
          "https://**",
          "https://127.0.0.1/**"
        ]
      },
      "fs": {
        "all": true,
        "scope": ["**/**"]
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.letchepare.waiua",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "WAIUA RE",
        "maximized": true
      }
    ]
  }
}
